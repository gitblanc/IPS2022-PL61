/*C87*/SET SCHEMA PUBLIC
drop table monitor
drop table no_socio
drop table recursosactividad
drop table cursillo
drop table lista_espera
drop table administracion
drop table actividadsocio
drop table alquileres
drop table socio
drop table TipoActividad
drop table recurso
drop table recursosALlevarSocio
drop table instalacion
create table socio(id_s varchar(10) primary key, nombre_s varchar(20) not null, apellidos_s varchar(50) not null, correo_s varchar(50) unique  not null, contrase\u00f1a_s varchar(50) not null)
create table monitor(id_m varchar(10) primary key, nombre_m varchar(20) not null, apellidos_m varchar(50) not null, correo_m varchar(50) not null )
create table no_socio(id_ns varchar(10) primary key, nombre_ns varchar(20) not null, apellidos_ns varchar(50) not null, correo_ns varchar(50) not null)
create table instalacion(nombre_i varchar(20) primary key, plazas int not null, cerrada_para_alquileres varchar(10))
create table recurso(nombre_r varchar(20) primary key, cantidad_r int check(cantidad_r >= 0), nombre_i varchar(20) foreign key references Instalacion(nombre_i))
INSERT INTO RECURSO VALUES('sin recursos',0,NULL)
