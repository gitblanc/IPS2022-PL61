CREATE MEMORY TABLE PUBLIC.SOCIO(ID_S VARCHAR(10) PRIMARY KEY,NOMBRE_S VARCHAR(20) NOT NULL,APELLIDOS_S VARCHAR(50) NOT NULL,CORREO_S VARCHAR(50) NOT NULL,CONTRASE\u00d1A_S VARCHAR(50) NOT NULL,UNIQUE(CORREO_S))
CREATE MEMORY TABLE PUBLIC.MONITOR(ID_M VARCHAR(10) PRIMARY KEY,NOMBRE_M VARCHAR(20) NOT NULL,APELLIDOS_M VARCHAR(50) NOT NULL,CORREO_M VARCHAR(50) NOT NULL)
CREATE MEMORY TABLE PUBLIC.NO_SOCIO(ID_NS VARCHAR(10) PRIMARY KEY,NOMBRE_NS VARCHAR(20) NOT NULL,APELLIDOS_NS VARCHAR(50) NOT NULL,CORREO_NS VARCHAR(50) NOT NULL)
CREATE MEMORY TABLE PUBLIC.INSTALACION(NOMBRE_I VARCHAR(20) PRIMARY KEY,PLAZAS INTEGER NOT NULL)
CREATE MEMORY TABLE PUBLIC.RECURSO(NOMBRE_R VARCHAR(20) PRIMARY KEY,CANTIDAD_R INTEGER,NOMBRE_I VARCHAR(20),CHECK(PUBLIC.RECURSO.CANTIDAD_R>=0),FOREIGN KEY(NOMBRE_I) REFERENCES PUBLIC.INSTALACION(NOMBRE_I))
CREATE MEMORY TABLE PUBLIC.TIPOACTIVIDAD(ID_A VARCHAR(150) PRIMARY KEY,TIPO VARCHAR(30) NOT NULL,INTENSIDAD VARCHAR(10),ACCESO VARCHAR(20),HORA_INICIO VARCHAR(6),HORA_FIN VARCHAR(6),NOMBRE_I VARCHAR(20),FECHA VARCHAR(20),PLAZAS_A INTEGER,CHECK(((PUBLIC.TIPOACTIVIDAD.INTENSIDAD='alta') OR (PUBLIC.TIPOACTIVIDAD.INTENSIDAD='moderada')) OR (PUBLIC.TIPOACTIVIDAD.INTENSIDAD='baja')),CHECK((PUBLIC.TIPOACTIVIDAD.ACCESO='libre') OR (PUBLIC.TIPOACTIVIDAD.ACCESO='reserva')),FOREIGN KEY(NOMBRE_I) REFERENCES PUBLIC.INSTALACION(NOMBRE_I))
CREATE MEMORY TABLE PUBLIC.CURSILLO(ID_C VARCHAR(10) PRIMARY KEY,NOMBRE_C VARCHAR(20) NOT NULL,DURACION VARCHAR(20) NOT NULL,HORARIO DATE NOT NULL)
CREATE MEMORY TABLE PUBLIC.LISTA_ESPERA(ID_LISTA VARCHAR(10) PRIMARY KEY,CANTIDAD INTEGER NOT NULL)
CREATE MEMORY TABLE PUBLIC.ADMINISTRACION(ID_A VARCHAR(10) PRIMARY KEY,NOMBRE_A VARCHAR(20) NOT NULL,APELLIDOS_A VARCHAR(50) NOT NULL,CORREO_A VARCHAR(50) NOT NULL)
CREATE MEMORY TABLE PUBLIC.RECURSOSACTIVIDAD(ID_A VARCHAR(150),NOMBRE_R VARCHAR(20),FOREIGN KEY(ID_A) REFERENCES PUBLIC.TIPOACTIVIDAD(ID_A),FOREIGN KEY(NOMBRE_R) REFERENCES PUBLIC.RECURSO(NOMBRE_R))
CREATE MEMORY TABLE PUBLIC.ACTIVIDADSOCIO(CORREO_S VARCHAR(50),ID_A VARCHAR(150),FOREIGN KEY(CORREO_S) REFERENCES PUBLIC.SOCIO(CORREO_S),FOREIGN KEY(ID_A) REFERENCES PUBLIC.TIPOACTIVIDAD(ID_A))
CREATE MEMORY TABLE PUBLIC.ALQUILERES(ID_A VARCHAR(100) PRIMARY KEY,NOMBRE_I VARCHAR(20),ID_SOCIO VARCHAR(10),FECHA VARCHAR(20),HORA_INICIO VARCHAR(6),HORA_FIN VARCHAR(6),FOREIGN KEY(NOMBRE_I) REFERENCES PUBLIC.INSTALACION(NOMBRE_I),FOREIGN KEY(ID_SOCIO) REFERENCES PUBLIC.SOCIO(ID_S))
